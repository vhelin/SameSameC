
.define TWO 2
.define THREE TWO+1

int8 global_var = 123+THREE;

void main() {
  int8 value = 8 >> 1;
  int8 j = 0;
  int8 k = 0;

  if (value > 7+2-1-1) {
     value = 3+2+1;
  }
  else if (value < 2) {
     if (value == 0) {
        value = 1;
     }
     else if (value == 1) {
        value = 0;
     }
  }
  else {
     value = 1+2;
  }

  j = k++;
  j = k--;
  j = ++k;
  j = (--k + 1) - 1;
  ++j;
  --j;
  j++;
  j--;

  return;

  j = 0;
  while (j < 16) {
      j++;
  }

  value = sum_2(value, 1);
  sum_1((TWO+1)*(TWO+TWO));
